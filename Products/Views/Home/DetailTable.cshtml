@model List<Products.Models.DetailTableModel>
@{
    ViewBag.Title = "DetailTable";
}

<h2>DetailTable</h2>

<table class="table-bordered col-12 table-hover">
    <thead>
        <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Sources</th>
    </tr>
    </thead>
    <tbody>
        @foreach (var el in Model)
        {
        <tr>
            <td>@Html.Partial("ProductPartialView",el.Product)</td>
            <td>@Html.Partial("DescPartialView", el.Descriptions)</td>
            <td>@Html.Partial("PropPartialView", el.Properties)</td>
        </tr>
        }
    
    </tbody>
</table>

